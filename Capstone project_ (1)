   risk_score = 0\n","\n","    # Age\n","    if row['Age'] >= 60:\n","        risk_score += 3\n","    elif row['Age'] >= 45:\n","        risk_score += 1\n","\n","    # Blood Pressure\n","    bp_parts = row['Blood Pressure'].split('/')\n","    systolic = int(bp_parts[0])\n","    diastolic = int(bp_parts[1])\n","\n","    if systolic >= 140 or diastolic >= 90:\n","        risk_score += 4 # High Blood Pressure\n","    elif systolic >= 130 or diastolic >= 85:\n","        risk_score += 2 # Elevated Blood Pressure\n","\n","    # Cholesterol\n","    if row['Cholesterol'] >= 240:\n","        risk_score += 3\n","    elif row['Cholesterol'] >= 200:\n","        risk_score += 1\n","\n","    # Smoking\n","    if row['Smoking']:\n","        risk_score += 5\n","\n","    # Family History\n","    if row['Family History']:\n","        risk_score += 2\n","\n","    # Determine risk category based on score\n","    if risk_score >= 10:\n","        return 'High'\n","    elif risk_score >= 5:\n","        return 'Moderate'\n","    else:\n","        return 'Low'\n","\n","# Apply the function to the DataFrame\n","df_heart_health['Risk_Assessment'] = df_heart_health.apply(assess_heart_risk, axis=1)\n","\n","print(\"DataFrame with Risk Assessment:\")\n","print(df_heart_health.head())"],"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["DataFrame with Risk Assessment:\n","   Age Blood Pressure  Cholesterol  Smoking  Family History Risk_Assessment\n","0   45         120/80          200    False           False             Low\n","1   60         140/90          240     True            True            High\n","2   52         130/85          210    False           False             Low\n","3   38         110/70          180    False            True             Low\n","4   70         150/95          260     True            True            High\n"]}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":89},"id":"fd057bf4","executionInfo